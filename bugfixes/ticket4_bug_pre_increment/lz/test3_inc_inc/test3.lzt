//[bug 4] pre-increment is not handled by semantic dispatch << c = *++fmt; >>
//2. ++++x
#include <testOutput.lzt>

int main()
	int x = 10;
	writePort(0, 0);
	writePort(1, 0);
	writePort(2, 0);
	
	writePort(0, x); 		//should be 10
	writePort(1, ++++x);	//should be ++(++x) which is err (++x is lvalue)
	writePort(2, x);		//should be 12
end
